##TO ADD A COLUMN
ALTER TABLE BACHA
ADD COLUMN AGE INT;

#TO DELETE A COLUMN
ALTER TABLE BACHA
DROP COLUMN AGE;

TO RENAME TABLE NAME
ALTER TABLE BACHA
RENAME TO BABULOG; 

TO CHANGE COLUMN NAME
ALTER TABLE BACHA
CHANGE COLUMN CITY(OLD NAME) PLACE(NEW NAME) VARCHAR(50);

TRUNCATE WILL DELETE THE TABLE DATA
