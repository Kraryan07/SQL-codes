GENERAL ORDER TO WRITE THE CLAUSE

SELECT COLUMN
FROM COLUMN
WHERE COLUMN
GRUOP BY COLUMN
HAVING CONDITION
ORDER BY COLUMN

SELECT CITY,COUNT(NAME)
FROM BACHA
WHERE CITY="DELHI"
GROUP BY CITY
HAVING MAX(MARKS)>20
ORDER BY CITY ASC;

UPDATE THE TABLE

UPDATE BACHA
SET CITY="KOLKATA"
WHERE CITY="ODIA"

TO DELETE FROM TABLE

DELETE FROM BACHA
WHERE MARKS<30
